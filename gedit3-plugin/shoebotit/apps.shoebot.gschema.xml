<?xml version="1.0" encoding="UTF-8"?>
<!-- http://zderadicka.eu/gsettings-flexible-configuration-system/ -->
<schemalist>
 
  <schema id="apps.shoebot.gedit"  path="/apps/shoebot/gedit/">

    <key type="s" name="current-virtualenv">
        <default>"python"</default>
        <summary>Python Interpreter</summary>
        <description>"python" will use first python in PATH</description>
    </key>
  
    <key name="virtualenvs" type="as">
        <default>[]</default>
        <summary>User Added Python Interpreters</summary>
        <description></description>
    </key>

    <key type="b" name="live-code">
      <default>false</default>
      <summary>Enable Live Code</summary>
      <description>Experimenal Live Code Mode.</description>
    </key>

  </schema>
</schemalist>
